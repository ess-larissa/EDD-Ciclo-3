# include <iostream>
#include <stack>
#include <vector>

using namespace std;

struct Node {
  int data;
  Node *left;
  Node *right;
};

Node *newNode(int data) {
  Node *node = new Node;
  node->data = data;
  node->left = NULL;
  node->right = NULL;
  return node;
}

void printInOrder(Node *node) {
  if (node == NULL) {
    return;
  }
  printInOrder(node->left);
  cout << node->data << " ";
  printInOrder(node->right);
}

